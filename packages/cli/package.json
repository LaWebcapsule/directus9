{
  "name": "@db-studio/cli",
  "version": "1.1.0",
  "description": "The official DB Studio CLI",
  "repository": "pxslip/cli",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "studioctl": "./bin/studio.js",
    "studio-ctl": "./bin/studio.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run cleanup",
    "build": "tsc --project ./tsconfig.json",
    "cleanup": "run-p cleanup:*",
    "cleanup:dist": "rimraf ./dist",
    "cleanup:coverage": "rimraf ./coverage",
    "profile": "clinic flame -- node ./bin/directus",
    "lint": "eslint src",
    "test": "echo \"No tests exist yet\" && exit 0"
  },
  "keywords": [
    "api",
    "client",
    "cms",
    "db-studio",
    "headless",
    "javascript",
    "node",
    "cli"
  ],
  "author": "Px Slip (Will K)<db-studio@pxslip.com>",
  "license": "MIT",
  "dependencies": {
    "@db-studio/format-title": "workspace:*",
    "@db-studio/sdk": "workspace:*",
    "app-module-path": "^2.2.0",
    "chalk": "^4.1.2",
    "cli-highlight": "^2.1.11",
    "cli-table3": "^0.6.5",
    "cli-ux": "^5.6.7",
    "cliui": "^8.0.1",
    "cosmiconfig": "^7.1.0",
    "csv": "^5.5.3",
    "debug": "^4.3.5",
    "dot-object": "^2.1.5",
    "dotenv": "^10.0.0",
    "eventemitter2": "^6.4.9",
    "figlet": "^1.7.0",
    "fs-extra": "^10.1.0",
    "fs-jetpack": "^5.1.0",
    "gluegun": "^5.1.6",
    "jaro-winkler": "^0.2.8",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "liquidjs": "^9.43.0",
    "marked": "^4.3.0",
    "marked-terminal": "^5.2.0",
    "merge-options": "^3.0.4",
    "open": "^8.4.2",
    "pretty-ms": "^7.0.1",
    "redent": "^3.0.0",
    "shimmer": "^1.2.1",
    "strip-ansi": "^6.0.1",
    "yargs": "^17.7.2",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@db-studio/config": "workspace:*",
    "@types/figlet": "1.5.4",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.0.3",
    "@types/js-yaml": "4.0.5",
    "@types/marked": "4.0.1",
    "@types/marked-terminal": "^3.1.7",
    "@types/node": "^18.19.41",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "27.3.1",
    "lint-staged": "^13.3.0",
    "nock": "13.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "^5.5.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
}
