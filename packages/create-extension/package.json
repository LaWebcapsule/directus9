		"url": "https://github.com/pxslip/db-studio.git",
		"directory": "packages/create-directus-extension"
	},
	"funding": "https://github.com/pxslip/db-studio?sponsor=1",
	"license": "GPL-3.0-only",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"main": "lib/index.js",
	"bin": {
		"cdbse": "./lib/index.js",
		"create-extension": "./lib/index.js"
	},
	"files": [
		"lib/index.js"
