	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@db-studio/config": "workspace:*",
		"@db-studio/types": "workspace:*",
		"@types/lodash-es": "4.17.7",
		"@vitest/coverage-c8": "0.30.1",
		"@vue/test-utils": "2.4.6",
		"axios": "1.3.6",
		"typescript": "5.0.4",
		"vitest": "0.30.1"
	},
	"dependencies": {
		"@db-studio/constants": "workspace:*",
		"@db-studio/utils": "workspace:*",
		"lodash-es": "4.17.21",
		"nanoid": "4.0.2",
		"vue": "3.4.29"
	}
