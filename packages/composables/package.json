{
	"name": "@db-studio/composables",
	"version": "1.0.3",
	"type": "module",
	"scripts": {
		"build": "tsc --build",
		"dev": "tsc --watch",
		"test": "vitest --watch=false"
	},
	"description": "Shared Vue composables for DB Studio (formerly Directus v9) use",
	"repository": {
		"type": "git",
		"url": "https://github.com/pxslip/db-studio.git",
		"directory": "packages/composables"
	},
	"funding": "https://github.com/pxslip/db-studio?sponsor=1",
	"license": "GPL-3.0",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"main": "dist/index.js",
	"files": [
		"dist",
		"!**/*.test.{js,d.ts}"
